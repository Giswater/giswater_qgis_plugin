[general]
folder_path = 'c:/dades'

[text_replace]
#for project (last lines)
"<ProjectCRSProj4String type="QString">+proj=utm +zone=31 +ellps=GRS80 +towgs84=0,0,0,0,0,0,0 +units=m +no_defs</ProjectCRSProj4String>", "<ProjectCRSProj4String type="QString">__PROJ4__</ProjectCRSProj4String>"
"<ProjectCrs type="QString">EPSG:25831</ProjectCrs>", "<ProjectCrs type="QString">__AUTHID__</ProjectCrs>"
"<ProjectionsEnabled type="int">1</ProjectionsEnabled>", "<ProjectionsEnabled type="int">1</ProjectionsEnabled>"

#for each geo layer
"dbname='giswater3'", "dbname='__DBNAME__'"
"host=localhost", "host=__HOST__"
"port=5432", "port=__PORT__"
"user='abcd'" "user='__USER__'" 
"password='1234'", "password='__PASSWORD__'"
"table=&quot;ud_sample&quot;", table=&quot;__SCHEMA_NAME__&quot;

#for each layer
"srid=25831", "srid=__SRID__"


[xml_set]
#for project (canvas extent). Only done for 1st found
<xmin></xmin>, "__XMIN__"
<ymin></ymin>, "__YMIN__"
<xmax></xmax>, "__XMAX__"
<ymax></ymax>, "__YMAX__"

#for each geo layer, in except of first found (in order to don't replace done before)
<extent></extent>, ""

#for each layer
<proj4></proj4>, "__PROJ4__"
<srsid></srsid>, "__SRSID__"
<srid></srid>, "__SRID__"
<authid></authid>, "__AUTHID__"
<description></description>, "__DESCRIPTION__"
<projectionacronym></projectionacronym>, "__PROJECTIONACRONYM__"
<ellipsoidacronym></ellipsoidacronym>, "__ELLIPSOIDACRONYM__"
<geographicflag></geographicflag>, "__GEOGRAPHICFLAG__"